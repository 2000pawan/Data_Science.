class Solution:
    def searchRange(self, nums, target):
        l=[]
        for _ in range(0,2):
            if target not in nums:
                l.append(-1)
        for _ in range(0,2):
            if target in nums:
                l.append(nums.index(target))
                nums.remove(target)
        return l


r=Solution()
